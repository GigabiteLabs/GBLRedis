  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GigabiteLabs/GBLRedis/issues"
  },
  "homepage": "https://github.com/GigabiteLabs/GBLRedis#readme",
  "dependencies": {
    "jsdoc": "^3.6.6",
    "log4js": "^6.1.2",
    "redis": "^3.0.2"
  }
}
