  "description": "A Redis DB client for sharing a Redis DB instance among several Node.js applications.",
  "main": "index.js",
  "scripts": {
    "test": "node tests/test.js",
    "test:setup": "bash tests/setup.sh",
    "test:teardown": "bash tests/teardown/teardown.sh",
